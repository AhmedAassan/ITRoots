@model ITRoots.BL.Models.ResetPasswordVM
@{
    ViewData["Title"] = "Reset Password";
    Layout = "~/Views/Shared/_Auth.cshtml";
}



    <div class="agileits-top">
        <form asp-controller="Account" asp-action="ResetPassword">
            <div asp-validation-summary="All"></div>
            <input  asp-for="Email" type="hidden"/>
            <input  asp-for="Token" type="hidden"/>
            <input class="text" type="password" asp-for="Password" placeholder="Password" required="">
            <input class="text w3lpass" type="password" asp-for="ConfirmPassword" placeholder="Confirm Password" required="">
            <input type="submit" value="ResetPassword">
        </form>
    </div>


